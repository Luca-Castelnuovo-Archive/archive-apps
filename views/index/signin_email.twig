<form id="auth-signin-email" class="modal">
    <div class="modal-content">
        <h4>Signin with email</h4>
        <div class="row mb-0">
            <div class="input-field col s12 mb-0">
                <input id="email" name="email" type="email" class="validate" required value="c19e6067-cc6e-4ef7-9b15-107890b63e88@usehelo.cloud">
                <label for="email">Email</label>
                <span class="helper-text" data-error="Valid email is required"></span>
