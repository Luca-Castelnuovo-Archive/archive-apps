<form id="auth-register-license" class="modal">
    <div class="modal-content">
        <h4>License Code</h4>
        <div class="row">
            <div class="input-field col s12">
                <input id="license_code" name="license_code" type="text" class="validate" required>
                <label for="license_code">License Code</label>
                <span class="helper-text" data-error="License code is required"></span>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                {% include 'partials/captcha.twig' %}
            </div>
        </div>
    </div>
    <div class="modal-footer">
        <button class="btn blue accent-4 waves-effect waves-light" type="submit">Submit</button>
    </div>
</form>
